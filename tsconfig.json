  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist"
  },
  "exclude": [
    "node_modules",
    "build.ts",
    "public",
    "analyze.test.ts"
  ]
